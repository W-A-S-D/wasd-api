generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  password  String
  acess     Int
  avatar    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("user")
}
